package fileAttente;

public class Personne {
	private String nom;
	private int age;
	public Personne(){};
	public Personne (String n, int a){
		nom=n;
		age=a;
	}
	public String getNom() {
		return nom;
	}
	public void setNom(String nom) {
		this.nom = nom;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String toString(){
		return nom+" "+age;
	}
	
}
